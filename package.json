{
  "dependencies": {
    "@arcjet/inspect": "1.1.0",
    "@arcjet/sveltekit": "1.1.0",
    "@fontsource-variable/figtree": "5.2.10",
    "@fontsource/ibm-plex-mono": "5.2.7",
    "@oddbird/css-anchor-positioning": "0.8.0",
    "zod": "4.3.5"
  },
  "devDependencies": {
    "@sveltejs/adapter-node": "5.4.0",
    "@sveltejs/kit": "2.50.0",
    "@sveltejs/vite-plugin-svelte": "6.2.1",
    "prettier": "3.7.4",
    "prettier-plugin-svelte": "3.4.1",
    "svelte": "5.47.1",
    "svelte-check": "4.3.5",
    "typescript": "5.9.3",
    "vite": "7.3.0"
  },
  "name": "@arcjet-example/sveltekit",
  "overrides": {
    "@sveltejs/kit": {
      "cookie": "0.7.0"
    }
  },
  "prettier": {
    "plugins": [
      "prettier-plugin-svelte"
    ],
    "overrides": [
      {
        "files": "*.svelte",
        "options": {
          "parser": "svelte"
        }
      }
    ]
  },
  "repository": "github:arcjet/example-sveltekit",
  "scripts": {
    "build": "vite build",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "dev": "vite dev --host",
    "prepare": "svelte-kit sync || echo ''",
    "start": "vite preview --host"
  },
  "type": "module",
  "version": "0.0.0"
}
